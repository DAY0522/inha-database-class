<h1>{{title}}</h1>

<table>
  <tr>
    <td>id</td>
    <td>Name</td>
    <td>Professor</td>
  </tr>
  {{#each inhaClass}}
    <form method="post">
      <tr>
        <td>{{id}}</td>
        <td>{{Name}}</td>
        <td>{{Professor}}</td>
        <td style="border: none; margin-left: 10px;">
          <button
            style="margin-left: 10px;"
            name="delBtn"
            type="submit"
            value="{{id}}"
            formaction="/delete/class"
          >
            delete
          </button>
        </td>
      </tr>
    </form>
  {{/each}}
</table>